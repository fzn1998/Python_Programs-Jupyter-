    "def cd(arr):\n",
    "    mp=100\n",
    "    for i in arr:\n",
    "        dis_price=mp*(100-i)/100\n",
    "        mp=dis_price\n",
    "    dis_perc=100-(dis_price)\n",
    "    return dis_perc\n",
    "\n"
   ]
  },
