    "def one_four(text):\n",
    "    first=text[0]\n",
    "    inbetween=text[1:3]\n",
    "    fourth=text[3]\n",
    "    rest=text[4:]\n",
    "    \n",
    "    return first.upper() +inbetween+fourth.upper() +rest"
